<Multi_key> <Cyrillic_softsign>    	: "ѣ"   U1123 			# строчная "ять"
<Multi_key> <Cyrillic_SOFTSIGN> 	: "Ѣ"   U1122 			# прописная "ять"
<Multi_key> <Cyrillic_ef>    		: "ө"   U04E8 			# "фита" ("крил. O c гор. чертой", реальная кир. фита - U0472) 
